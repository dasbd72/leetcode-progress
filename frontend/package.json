{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts,.html",
    "format": "eslint . --ext .ts,.html --fix && prettier --write .",
    "prepare": "cd .. && husky frontend/.husky && git config --unset-all core.hooksPath"
  },
  "private": true,
  "type": "module",
  "lint-staged": {
    "*.{js,ts,html,css,scss,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/cdk": "^19.2.6",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.6",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@aws-amplify/backend": "^1.15.0",
    "@aws-amplify/ui-angular": "^5.0.40",
    "@tailwindcss/cli": "^4.0.15",
    "@tailwindcss/postcss": "^4.0.15",
    "angular-auth-oidc-client": "19.0.0",
    "aws-amplify": "^6.14.0",
    "chart.js": "^4.4.8",
    "ng2-charts": "^8.0.0",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
